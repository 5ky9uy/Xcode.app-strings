#!/bin/sh
prefix='/System/Volumes/Data/SWE/Apps/DTs2/ActiveBuildRoot/Library/Caches/com.apple.xbs/Sources/XCSCouchDB/XCSCouchDB-21.0.4/build'
mozilla_version=''
LIBRARY_NAME='mozjs'
NSPR_CFLAGS=''
JS_CONFIG_LIBS=' -dynamiclib -install_name @executable_path/libmozjs.dylib -compatibility_version 1 -current_version 1 -single_module  -lm '
MOZ_JS_LIBS='-Llib -lmozjs'
usage()
cat <<EOF
Usage: js-config [OPTIONS]
Options:
[--prefix[=DIR]]
[--exec-prefix[=DIR]]
[--includedir[=DIR]]
[--libdir[=DIR]]
[--version]
[--libs]
[--cflags]
[--lib-filenames]
exit $1
if test $# -eq 0; then
usage 1 1>&2
while test $# -gt 0; do
  case "$1" in
  -*=*) optarg=`echo "$1" | sed 's/[-_a-zA-Z0-9]*=//'` ;;
  *) optarg= ;;
  esac
  case $1 in
    --prefix=*)
      prefix=$optarg
      ;;
    --prefix)
      echo_prefix=yes
      ;;
    --exec-prefix=*)
      exec_prefix=$optarg
      ;;
    --exec-prefix)
      echo_exec_prefix=yes
      ;;
    --includedir=*)
      includedir=$optarg
      ;;
    --includedir)
      echo_includedir=yes
      ;;
    --libdir=*)
      libdir=$optarg
      ;;
    --libdir)
      echo_libdir=yes
      ;;
    --version)
      echo "$mozilla_version"
      ;;
    --cflags)
      echo_cflags=yes
      ;;
    --libs)
      echo_libs=yes
      ;;
    *)
      usage 1 1>&2
      ;;
  esac
  shift
done
# Set variables that may be dependent upon other variables
if test -z "$exec_prefix"; then
    exec_prefix=/System/Volumes/Data/SWE/Apps/DTs2/ActiveBuildRoot/Library/Caches/com.apple.xbs/Sources/XCSCouchDB/XCSCouchDB-21.0.4/build
if test -z "$includedir"; then
    includedir=include
if test -z "$libdir"; then
    libdir=lib
if test "$echo_prefix" = "yes"; then
    echo $prefix
if test "$echo_exec_prefix" = "yes"; then
    echo $exec_prefix
if test "$echo_includedir" = "yes"; then
    echo $includedir
if test "$echo_libdir" = "yes"; then
    echo $libdir
if test "$echo_cflags" = "yes"; then
    echo "-I$includedir/js $NSPR_CFLAGS"
if test "$echo_libs" = "yes"; then
    echo "$MOZ_JS_LIBS $JS_CONFIG_LIBS"
fi      
