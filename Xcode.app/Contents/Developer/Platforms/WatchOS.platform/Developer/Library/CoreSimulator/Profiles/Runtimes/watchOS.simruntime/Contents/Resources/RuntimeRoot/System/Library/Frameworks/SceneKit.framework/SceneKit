    float4x4        viewToCubeTransform;
#endif
    float zfar;
} background_uniforms_t;
typedef struct {
#ifdef __SIMD_HEADER__
    matrix_float4x2 texcoords;
#else
    float4x2        texcoords;
#endif
} background_video_uniforms_t;
